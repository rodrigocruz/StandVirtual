{
  "name": "rodrigocruz/stand-virtual",
  "description": "Stand Virtual API wrapper",
  "keywords": [
    "standvirtual"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Rodrigo Cruz",
      "email": "labrugice@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "laravel/pint": "^1.7",
    "pestphp/pest": "^2.2",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "Rodrigocruz\\StandVirtual\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
